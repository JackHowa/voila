<h1>Master Registration</h1>


<%= form_for(:user, url: users_path) do |f| %>
  <%= f.label :stage_name, "Stage Name" %>
  <%= f.text_field :stage_name %>

  <%#= f.label :secret, "Secret Key" %>
  <%#= text_field_tag, :secret %>

  <%= f.label :email, "Email" %>
  <%= f.text_field :email %>

  <%= f.label :password, "Password" %>
  <%= f.text_field :password %>

  <%= f.submit %>
<% end %>

